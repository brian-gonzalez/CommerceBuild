{
  "root": true,
  "env": {
    "amd": true,
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  },
  "extends": ["airbnb-base"],
  "rules": {
    "func-names": "off",
    "import/no-extraneous-dependencies": ["error", { "devDependencies": true }],
    "import/no-unresolved": "off",
    "no-underscore-dangle": "off"
  }
}
