{
  "ignore": ["app_storefront_base"],
  "rules": {
    "enforce-require": {},
    "no-git-conflict": {},
    "no-import-package": {},
    "no-inline-style": {
      "level": "warn"
    },
    "no-space-only-lines": {},
    "no-tabs": {},
    "no-trailing-spaces": {},
    "max-lines": {},
    "no-redundant-context": {},
    "leading-iscontent": {},
    "max-depth": {},
    "no-embedded-isml": {
      "level": "warn"
    },
    "no-hardcode": {},
    "no-require-in-loop": {},
    "one-element-per-line": {
      "level": "warn"
    },
    "leading-iscache": {},
    "no-deprecated-attrs": {},
    "contextual-attrs": {},
    "enforce-security": {
      "level": "warn"
    }
  }
}
